{
  "expo": {
    "name": "ChatApp",
    "slug": "ChatApp",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/chat.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/chat.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chat.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "resizeMode": "native"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "ChatApp for general use",
    "githubUrl": "https://github.com/Akash-Manikandan/ChatApp"
  }
}
